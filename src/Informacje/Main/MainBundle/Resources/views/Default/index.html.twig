<div id="content" style="clear:left">
{# WIADOMOŚCI  #}
<div class="wiadomości" ><h2>Wiadomości </h2>
<table class="tabela">{% for wiadomość in wiadomości | reverse if  loop.index < 11 %}<tr>
<td style="width:80px; height:50px;"><div id="mini" style="width:70px;height:45px; background-image: url('{{ asset('uploads/documents') }}/{{ wiadomość.path1 }}') ; 
background-size:contain; background-repeat: no-repeat; background-position : center  " > &nbsp;</div> </td>
<td style="width:200px; height:50px;"><a href="{{ path('informacje_main_main_default_strona', { 'slug': wiadomość.id  }) }}">{{ wiadomość.header1 }}
</a></td></tr>
{% endfor %}
</table></div>

{# CAROUSEL #}
<div id='carousel' style="float:left ; margin-bottom : 5px;">
<h2>Rozmaitości </h2>
<div id='carousel_container'>
  <div id='left_scroll'><img src="{{ asset('img/left.png') }}" /></div>
  <div id='carousel_inner'>
        <ul id='carousel_ul'>
		{% for rozmaitosc in rozmaitosci | reverse %}
            <li><a href="{{ path('informacje_main_main_default_strona', { 'slug': rozmaitosc.id  }) }}">
			<img src="{{ asset('uploads/documents') }}/{{ rozmaitosc.path1 }}" />
			<span class="carousel_span">{{ rozmaitosc.header1 }}</span></a></li>
		{% endfor %}
        </ul>
  </div>
  <div id='right_scroll'><img src="{{ asset('img/right.png') }}" /></div>
</div>
</div>

{# REKLAMA #}
<div id="reklama"> 
