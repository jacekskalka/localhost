 <div class="spacer" style="clear: both;"></div> 

{# SLIDER #}
<div id="rozrywka" style="float:left; margin-top : 3px;">
<h2> Rozrywka </h2>
<div id="s3slider"  >
    <ul id="s3sliderContent">
        
		{% for rozrywki in rozrywka | reverse %}
		
		<li class="s3sliderImage">
			<a href="{{ path('informacje_main_main_default_strona', { 'slug': rozrywki.id  }) }}">
            <img style="width:650px; height:361px" src="{{ asset('uploads/documents') }}/{{rozrywki.path1 }}">
            <span>{{ rozrywki.header1 }}</span></a>
